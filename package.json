{
  "name": "webpack-react-cli",
  "version": "1.0.0",
  "description": "基于webpack的react脚手架",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config build/webpack.config.dev.js",
    "dist": "npm run clean && cross-env NODE_ENV=development webpack --config build/webpack.config.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js"
  },
  "keywords": [
    "webpack",
    "react",
    "cli",
    "hmr"
  ],
  "author": "jacintoface",
  "license": "ISC",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-actions": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "autoprefixer": "^7.2.6",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
